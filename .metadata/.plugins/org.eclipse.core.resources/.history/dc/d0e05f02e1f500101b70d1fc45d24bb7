package MultiThreading;

public class JoinCode {
	public static void main(String[] args) {
		Text t1 = new Text();
		Text t2 = new Text();

		t1.start();
//		t2.start();
	}
}

class Text extends Thread {
	public void run() {
		for (int i = 1; i <= 5; i++) {
			System.out.println(Thread.currentThread().getName() + " " + i);
		}
	}
}