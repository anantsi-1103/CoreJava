package FileHandling;

import java.io.File;

public class file1 {
	public static void main(String[] args) throws Exception {
		File f1 = new File("/Users/anantsi/Desktop/fileWriter.txt");
		
		System.out.println(f1.exists());
		
		f1.createNewFile();
		
		System.out.println(f1.exists());
		
		
	}
}
